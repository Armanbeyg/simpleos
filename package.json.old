		"moment": "^2.29.1",
		"ng-snotify": "^9.0.1",
		"ngx-echarts": "^5.2.1",
		"ngx-electron": "^2.2.0",
		"ngx-json-viewer": "^2.4.0",
		"ngx-lottie": "^6.4.0",
		"ngx-markdown": "^10.1.1",
		"ngx-order-pipe": "^2.1.1",
		"ngx-pagination": "^5.0.0",
		"node-fetch": "^2.6.1",
		"node-pre-gyp": "^0.15.0",
		"node-sass": "^4.14.1",
		"node-schedule": "^1.3.2",
		"open": "^7.3.0",
		"popper.js": "^1.16.1",
		"portfinder": "^1.0.28",
		"primeicons": "^4.0.0-rc.2",
		"primeng": "^9.1.3",
		"quill": "^1.3.7",
		"resize-observer-polyfill": "^1.5.1",
		"socket.io": "^2.3.0",
		"socket.io-client": "^2.3.1",
		"sudo-prompt": "^9.2.1",
		"tar": "^6.0.5",
		"text-mask-addons": "3.8.0",
		"tslib": "^2.0.3"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "0.1001.7",
		"@angular/cli": "10.1.7",
		"@angular/compiler": "10.1.6",
		"@angular/compiler-cli": "10.1.6",
		"@angular/language-service": "10.1.6",
		"@electron/typescript-definitions": "8.8.0",
		"@types/crypto-js": "4.0.1",
		"@types/echarts": "4.8.1",
		"@types/express": "4.17.8",
		"@types/node": "14.11.8",
		"@webcomponents/custom-elements": "^1.4.2",
		"codelyzer": "^6.0.1",
		"core-js": "^3.6.5",
		"electron": "9.3.2",
		"electron-builder": "22.9.1",
		"electron-builder-notarize": "^1.2.0",
		"electron-reload": "^1.5.0",
		"eslint": "^7.11.0",
		"npm-run-all": "^4.1.5",
		"protractor": "^7.0.0",
		"rxjs": "^6.6.3",
		"rxjs-compat": "^6.6.3",
		"text-encoding-shim": "^1.0.5",
		"ts-node": "^9.0.0",
		"typescript": "4.0.3",
		"wait-on": "^5.2.0",
		"webpack": "^4.44.2",
		"webpack-dev-server": "^3.11.0",
		"zone.js": "0.11.3"
	}
}

		"electron-reload": "^1.5.0",
		"eslint": "^7.17.0",
		"html-webpack-plugin": "^4.5.1",
		"npm-run-all": "^4.1.5",
		"protractor": "^7.0.0",
		"rxjs": "^6.6.3",
		"rxjs-compat": "^6.6.3",

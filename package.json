		"ngx-lottie": "^6.4.0",
		"ngx-markdown": "^11.0.1",
		"ngx-order-pipe": "^2.1.1",
		"ngx-pagination": "^5.0.0",
		"node-abi": "^2.19.3",
		"node-fetch": "^2.6.1",
		"node-gyp": "^7.1.2",
		"node-hid": "^2.1.1",
		"node-pre-gyp": "^0.17.0",
		"node-schedule": "^1.3.2",
		"open": "^7.3.1",
		"popper.js": "^1.16.1",
		"portfinder": "^1.0.28",

{
	"name": "simpleos",
	"version": "1.0.6",
	"productName": "simpleos",
	"appId": "io.eosrio.simpleos",
	"compilerVersion": "DEFAULT",
	"description": "EOSIO Blockchain Interface & Wallet",
	"author": {
		"name": "EOS Rio",
		"email": "contact@eosrio.io"
	},
	"repository": "https://github.com/eosrio/simpleos",
	"license": "MIT",
	"scripts": {
		"config:default": "node configurations/build-simpleos.js",
		"config:liberland": "node configurations/build-liberland.js",
		"electron:start": "npm run postinstall:electron && npm-run-all -p ng:serve electron:serve",
		"electron:serve": "wait-on http-get://localhost:7777/ && electron index.js --serve --devtools",
		"start:electron-dev": "electron index.js --devtools --debug",
		"start:electron-prod": "electron index.js",
		"start:electron-prod-automode": "electron index.js --autostart",
		"build": "ng build",
		"build:prod": "ng build --prod",
		"build:mac": "electron-builder build --mac --publish=never",
		"build:win": "electron-builder build --win --publish=never",
		"build:linux": "electron-builder build --linux --publish=never",
		"build:linux-snap": "electron-builder build --linux snap --publish=never",
		"ng": "ng",
		"ng:serve": "ng serve --port 7777",
		"pack": "electron-builder --dir",
		"dist": "electron-builder",
		"release:default": "npm run config:default && npm run build:prod && electron-builder",
		"release:liberland": "npm run config:liberland && npm run build:prod && electron-builder",
		"postinstall": "electron-builder install-app-deps && npm run postinstall:electron",
		"postinstall:electron": "node scripts/postinstall.js"
	},
	"engines": {
		"node": ">= 14.0.0",
		"npm": ">= 7.0.0"
	},
	"private": true,
	"main": "index.js",
	"build": {
		"appId": "simpleos",
		"directories": {
			"output": "dist",
			"buildResources": "build"
		},
		"files": [
			"index.js",
			"ng-dist",
			"static",
			"resources",
			"electron_modules",
			"build/icons/**/*"
		],
		"afterSign": "electron-builder-notarize",
		"mac": {
			"hardenedRuntime": true,
			"entitlements": "scripts/entitlements.mac.plist",
			"category": "public.app-category.finance",
			"target": [
				"dmg"
			],
			"icon": "icons/simpleos/icon.icns"
		},
		"dmg": {
			"sign": false
		},
		"linux": {
			"executableName": "simpleos",
			"target": [
				"appImage",
				"deb"
			],
			"icon": "icons/simpleos",
			"category": "Network",
			"desktop": {
				"Name": "SimplEOS",
				"GenericName": "SimplEOS Wallet",
				"X-GNOME-FullName": "simpleos",
				"Comment": "SimplEOS Blockchain Wallet",
				"Type": "Application",
				"Terminal": "false",
				"StartupNotify": "false",
				"Categories": "Network;"
			}
		},
		"snap": {
			"confinement": "classic",
			"grade": "stable"
		},
		"win": {
			"target": [
				"nsis"
			],
			"icon": "icons/simpleos/icon.ico"
		},
		"appx": {
			"backgroundColor": "#232526",
			"languages": [
				"en-US"
			],
			"showNameOnTiles": true,
			"identityName": "EOSRio.SimplEOS"
		},
		"nsis": {
			"oneClick": true,
			"createDesktopShortcut": true
		},
		"protocols": [
			{
				"name": "simpleos",
				"role": "Viewer",
				"schemes": [
					"simpleos"
				]
			}
		]
	},
	"files": [
		"*.js",
		"build",
		"static",
		"resources",
		"ng-dist",
		"node_modules"
	],
	"dependencies": {
		"@ajsf/core": "0.3.0-beta.4",
		"@ajsf/material": "0.3.0-beta.4",
		"@angular/animations": "11.0.8",
		"@angular/cdk": "11.0.3",
		"@angular/common": "11.0.8",
		"@angular/core": "11.0.8",
		"@angular/flex-layout": "10.0.0-beta.32",
		"@angular/forms": "11.0.8",
		"@angular/material": "11.0.3",
		"@angular/material-moment-adapter": "11.0.3",
		"@angular/platform-browser": "11.0.8",
		"@angular/platform-browser-dynamic": "11.0.8",
		"@angular/router": "11.0.8",
		"@clr/angular": "^4.0.9",
		"@clr/core": "4.0.9",
		"@clr/icons": "4.0.9",
		"@clr/ui": "4.0.9",
		"@fortawesome/angular-fontawesome": "^0.8.1",
		"@fortawesome/fontawesome-svg-core": "^1.2.32",
		"@fortawesome/free-brands-svg-icons": "^5.15.1",
		"@fortawesome/free-regular-svg-icons": "^5.15.1",
		"@fortawesome/free-solid-svg-icons": "^5.15.1",
		"@fortawesome/pro-duotone-svg-icons": "^5.15.1",
		"@fortawesome/pro-light-svg-icons": "^5.15.1",
		"@fortawesome/pro-regular-svg-icons": "^5.15.1",
		"@fortawesome/pro-solid-svg-icons": "^5.15.1",
		"@ledgerhq/hw-transport": "5.38.0",
		"@ledgerhq/hw-transport-node-hid": "5.38.0",
		"@webcomponents/webcomponentsjs": "^2.5.0",
		"ajv": "^6.12.6",
		"angular2-text-mask": "9.0.0",
		"angular2-toaster": "11.0.1",
		"animate.css": "^4.1.1",
		"asn1-ber": "^1.1.0",
		"auto-launch": "^5.0.5",
		"bcrypt": "^5.0.0",
		"bip32-path": "^0.4.2",
		"body-parser": "^1.19.0",
		"bootstrap": "^4.5.3",
		"chart.js": "^2.9.4",
		"cors": "^2.8.5",
		"crypto-js": "^4.0.0",
		"echarts": "^4.9.0",
		"electron-context-menu": "^2.4.0",
		"eosjs": "^21.0.1-rc2",
		"express": "^4.17.1",

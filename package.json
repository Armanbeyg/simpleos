{
  "name": "simpleos",
  "productName": "simpleos",
  "description": "EOS Blockchain Interface & Wallet",
  "author": "EOS Rio",
  "repository": "https://github.com/eosrio/simpleos",
  "homepage": "https://github.com/eosrio/simpleos",
  "license": "UNLICENSED",
  "version": "0.2.3",
  "scripts": {
    "ng": "ng",
    "start": "electron-forge start",
    "serve": "ng serve --port 7878",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-watch": "ng build --prod --watch",
    "postinstall": "node patch.js",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "private": true,
  "main": "src/index.js",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "src/favicon.ico"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "platforms": [
            "win32"
          ],
          "config": {
            "name": "simplEOS",
            "title": "simplEOS",
            "loadingGif": "C:/simpleos/simpleos.gif",
            "setupIcon": "C:/simpleos/src/favicon.ico",
            "noMsi": false,
            "setupMsi": "simplEOS-setup.msi"
          }
        },
        {
          "name": "@electron-forge/maker-dmg",
          "platforms": [
            "darwin"
          ],
          "config": {
            "name": "simplEOS",
            "format": "UDZO"
          }
        },
        {
          "name": "@electron-forge/maker-deb",
          "platforms": [
            "linux"
          ],
          "config": {
            "options": {
              "name": "simplEOS",
              "productName": "simplEOS",
              "icon": "/home/entropia/simpleos/src/favicon.ico",
              "homepage": "https://github.com/eosrio/simpleos",
              "maintainer": "eosrio",
              "productDescription": "EOS Blockchain Interface & Wallet by EOS Rio",
              "description": "EOS Blockchain Interface & Wallet by EOS Rio"
            }
          }
        },
        {
          "name": "@electron-forge/maker-rpm",
          "platforms": [
            "linux"
          ],
          "config": {}
        }
      ]
    }
  },
  "dependencies": {
    "@angular/animations": "^6.0.4",
    "@angular/cdk": "^6.0.4",
    "@angular/common": "^6.0.4",
    "@angular/compiler": "^6.0.4",
    "@angular/core": "^6.0.4",
    "@angular/forms": "^6.0.4",
    "@angular/http": "^6.0.4",
    "@angular/material": "^6.0.4",
    "@angular/platform-browser": "^6.0.4",
    "@angular/platform-browser-dynamic": "^6.0.4",
    "@angular/router": "^6.0.4",
    "@clr/angular": "^0.12.0-rc.2",
    "@clr/icons": "^0.12.0-rc.2",
    "@clr/ui": "^0.12.0-rc.2",
    "@fortawesome/angular-fontawesome": "^0.1.0-10",
    "@fortawesome/fontawesome-pro-webfonts": "^1.0.9",
    "@fortawesome/fontawesome-svg-core": "^1.2.0-11",
    "@fortawesome/free-brands-svg-icons": "^5.1.0-8",
    "@fortawesome/pro-light-svg-icons": "^5.1.0-8",
    "@fortawesome/pro-regular-svg-icons": "^5.1.0-8",
    "@fortawesome/pro-solid-svg-icons": "^5.1.0-8",
    "@webcomponents/custom-elements": "^1.0.0",
    "angular2-text-mask": "^9.0.0",
    "angular2-toaster": "^6.0.0",
    "bootstrap": "^4.1.1",
    "core-js": "^2.5.4",
    "electron-compile": "^6.4.2",
    "electron-squirrel-startup": "^1.0.0",
    "eosjs": "^14.0.0",
    "hammerjs": "^2.0.8",
    "jquery": "^3.3.1",
    "moment": "^2.22.2",
    "ng-lottie": "^0.3.1",
    "popper.js": "^1.14.3",
    "primeng": "^5.2.6",
    "rxjs": "^6.2.0",
    "opn": "^5.3.0",
    "rxjs-compat": "^6.2.0",
    "text-encoding-shim": "latest",
    "text-mask-addons": "^3.7.2",
    "update-electron-app": "^1.3.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.6.8",
    "@angular/cli": "^6.0.8",
    "@angular/compiler-cli": "^6.0.4",
    "@angular/language-service": "^6.0.4",
    "@electron-forge/cli": "^6.0.0-beta.1",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.1",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "10.1.2",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "codelyzer": "~4.2.1",
    "electron": "2.0.2",
    "electron-rebuild": "^1.7.3",
    "electron-reload": "^1.2.2",
    "electron-winstaller": "^2.6.4",
    "npm-run-all": "^4.1.3",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
